{
  "name": "@distube/soundcloud",
  "version": "1.3.1",
  "description": "SoundCloud plugin for DisTube.js.org",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "exports": "./dist/index.js",
  "directories": {
    "lib": "src"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --write \"**/*.{js,ts,json,yml,yaml,md}\"",
    "build": "tsup",
    "build:check": "tsc --noEmit",
    "update": "ncu -u && yarn up '**' -R",
    "postinstall": "husky install",
    "prepack": "yarn build && pinst -d",
    "postpack": "pinst -e"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/distubejs/soundcloud.git"
  },
  "keywords": [
    "distube",
    "plugin",
    "soundcloud",
    "discord.js",
    "discord-music-bot"
  ],
  "author": "Skick",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/distubejs/soundcloud/issues"
  },
  "homepage": "https://github.com/distubejs/soundcloud#readme",
  "devDependencies": {
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "@discordjs/opus": "^0.9.0",
    "@discordjs/voice": "^0.16.0",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "commitlint": "^17.6.1",
    "discord.js": "^14.9.0",
    "distube": "^4.0.4",
    "eslint": "^8.38.0",
    "eslint-config-distube": "^1.6.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-deprecation": "^1.4.1",
    "eslint-plugin-jsdoc": "^41.1.1",
    "husky": "^8.0.3",
    "nano-staged": "^0.8.0",
    "pinst": "^3.0.0",
    "prettier": "^2.8.7",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "soundcloud.ts": "^0.5.0"
  },
  "peerDependencies": {
    "distube": "3||4"
  },
  "nano-staged": {
    "*.ts": [
      "prettier --write",
      "eslint"
    ],
    "*.{json,yml,yaml}": [
      "prettier --write"
    ]
  }
}
