{
  "name": "@distube/soundcloud",
  "version": "1.1.0",
  "description": "Soundcloud plugin for DisTube.js.org",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --write \"**/*.{js,ts,json,yml,yaml,md}\"",
    "build": "tsup",
    "build:check": "tsc --noEmit",
    "npm:publish": "npm publish --access public",
    "postinstall": "husky install",
    "prepublishOnly": "pinst -d",
    "postpublish": "pinst -e"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/distubejs/soundcloud.git"
  },
  "keywords": [
    "distube",
    "plugin",
    "soundcloud",
    "discord.js",
    "discord-music-bot"
  ],
  "author": "Skick",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/distubejs/soundcloud/issues"
  },
  "homepage": "https://github.com/distubejs/soundcloud#readme",
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@discordjs/opus": "^0.8.0",
    "@discordjs/voice": "^0.11.0",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "commitlint": "^17.0.3",
    "discord.js": "^14.0.1",
    "distube": "^3.3.4",
    "eslint": "^8.20.0",
    "eslint-config-distube": "^1.6.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-deprecation": "^1.3.2",
    "eslint-plugin-jsdoc": "^39.3.3",
    "husky": "^8.0.1",
    "nano-staged": "^0.8.0",
    "pinst": "^3.0.0",
    "prettier": "^2.7.1",
    "tsup": "^6.1.3",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "soundcloud.ts": "^0.4.6"
  },
  "peerDependencies": {
    "distube": "3||4"
  },
  "nano-staged": {
    "*.ts": [
      "prettier --write",
      "eslint"
    ],
    "*.{json,yml,yaml}": [
      "prettier --write"
    ]
  }
}
