{
  "name": "@distube/soundcloud",
  "version": "0.2.2",
  "description": "Soundcloud plugin for DisTube.js.org",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
