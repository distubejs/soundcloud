    "build:check": "tsc --noEmit --incremental false",
    "npm:publish": "npm publish --access public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/distubejs/soundcloud.git"
  },
  "keywords": [
    "distube",
    "plugin",
    "soundcloud",
    "discord.js",
    "discord-music-bot"
  ],
  "author": "Skick",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/distubejs/soundcloud/issues"
  },
  "homepage": "https://github.com/distubejs/soundcloud#readme",
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@discordjs/opus": "^0.5.3",
    "commitlint": "^13.1.0",
    "discord.js": "^13.1.0",
    "distube": "^3.0.0-beta.34",
    "eslint": "^7.32.0",
    "eslint-config-distube": "^1.2.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.2",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "soundcloud.ts": "^0.4.1"
  },
  "peerDependencies": {
    "distube": "^3.0.0-beta.34"
  },
  "lint-staged": {
    "*.ts": [
